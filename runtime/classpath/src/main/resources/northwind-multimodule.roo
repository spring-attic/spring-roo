// Creating Northwind project
project setup --topLevelPackage org.springframework.roo.northwind --projectName Northwind --java 7 --multimodule

// Configuring settings
settings add --name spring.roo.jpa.require.schema-object-name --value true

// Configure persistence
jpa setup --provider HIBERNATE --database HYPERSONIC_PERSISTENT

// Create domain entities
module focus --moduleName model

// Temporary removed 'readOnly' feature
//entity jpa --class ~.City --table NW_CITIES --sequenceName NW_CITIES_CITY_ID_SEQ --identifierStrategy SEQUENCE --identifierColumn CITY_ID --versionField version --versionType long --versionColumn VERSION --readOnly
//entity jpa --class ~.Country --table NW_COUNTRIES --sequenceName NW_COUNTRIES_COUNTRY_ID_SEQ --identifierStrategy SEQUENCE --identifierColumn COUNTRY_ID --versionField version --versionType long --versionColumn VERSION --readOnly
